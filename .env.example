# QTunnel Server Configuration
# Copy this file to .env and customize the values

# REQUIRED: Change this to a secure random token
# Generate with: openssl rand -hex 32
QTUNNEL_AUTH_TOKEN=change-this-to-a-secure-random-token-32-characters-long

# REQUIRED: Your domain name where QTunnel server will be hosted
QTUNNEL_DOMAIN=yourdomain.com

# SSL Certificate resolver for Traefik (depends on your Traefik configuration)
# Common values: cloudflare, letsencrypt, http
CERT_RESOLVER=cloudflare

# Optional: External ports for direct access (useful for debugging)
# These are the ports exposed on your host machine
EXTERNAL_WS_PORT=8092
EXTERNAL_PROXY_PORT=8093

# Optional: Resource limits
MEMORY_LIMIT=256m
CPU_LIMIT=0.5